<div class="modal fade" id="userEditFromModal" tabindex="-1" role="dialog" aria-labelledby="userFromModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="userFromModalLabel">User Form</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class='row'>
          <div class='col-md-12'>
            <%= render 'form', user: user %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
